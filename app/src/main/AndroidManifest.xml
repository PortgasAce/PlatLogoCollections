<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  package="com.portgas.demo.activity">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    tools:ignore="GoogleAppIndexingWarning">

    <activity
      android:name="com.portgas.demo.activity.MainActivity"
      android:theme="@style/Theme.AppCompat.DayNight">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity
      android:configChanges="orientation|keyboardHidden"
      android:name=".PlatLogoActivityP"
      android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"/>

    <activity
      android:configChanges="orientation|keyboardHidden"
      android:name=".PlatLogoActivityO"
      android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"/>

    <activity
      android:configChanges="orientation|keyboardHidden"
      android:name=".PlatLogoActivityN"
      android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"/>

    <activity
      android:configChanges="orientation|keyboardHidden"
      android:name=".PlatLogoActivityM"
      android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"/>

    <activity
      android:configChanges="orientation|keyboardHidden"
      android:name=".PlatLogoActivityL"
      android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"/>

    <activity
      android:configChanges="orientation|keyboardHidden"
      android:name=".PlatLogoActivityK"
      android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"/>

    <activity
      android:configChanges="orientation|keyboardHidden"
      android:name=".PlatLogoActivityJ"
      android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen"/>

    <activity
      android:name=".PlatLogoActivityG"
      android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>

    <activity
      android:name=".PlatLogoActivityNoStyle"/>
    <activity
      android:theme="@style/WallpaperTheme"
      android:name=".PlatLogoActivityStyle"/>

  </application>

</manifest>